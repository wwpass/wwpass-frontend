{
  "name": "wwpass-frontend",
  "license": "MIT",
  "version": "3.0.4",
  "description": "Frontend WWPass JavaScript Library",
  "scripts": {
    "eslint": "eslint src/",
    "jest": "jest",
    "buildall": "bump patch && npm run build",
    "build": "rollup -c",
    "buildui": "rollup -c rollup.config.uionly.js",
    "test": "npx size-limit ./src/index.js && npm run eslint && npm run jest",
    "bump": "bump patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwpass/wwpass-frontend.git"
  },
  "keywords": [
    "auth",
    "2FA",
    "wwpass"
  ],
  "author": "WWPass",
  "main": "./src/lib",
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/preset-env": "7.3.4",
    "@size-limit/preset-small-lib": "^3.0.0",
    "@trust/webcrypto": "0.9.2",
    "babel-jest": "26.0.1",
    "babel-plugin-rewire": "1.2.0",
    "bump-version": "0.5.0",
    "del": "2.2.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "jest": "26.0.1",
    "jest-canvas-mock": "2.2.0",
    "regenerator-runtime": "0.12.0",
    "rollup": "1.6.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.1",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "^6.0.4",
    "text-encoding": "0.7.0"
  },
  "size-limit": [
    {
      "path": "./dist/wwpass-frontend.js",
      "limit": "18 Kb"
    }
  ],
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "node": "^14.3.0",
    "npm": "^6.14.5",
    "qrcode": "0.8.2"
  }
}
