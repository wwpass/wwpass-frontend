{
  "name": "wwpass-frontend",
  "license": "MIT",
  "version": "3.3.0",
  "description": "Frontend WWPass JavaScript Library",
  "scripts": {
    "eslint": "eslint src/ test/",
    "jest": "jest",
    "buildall": "bump patch && npm run build",
    "build": "webpack --mode=production",
    "test": "npx size-limit ./src/index.js && npm run eslint && npm run jest",
    "bump": "bump patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwpass/wwpass-frontend.git"
  },
  "keywords": [
    "auth",
    "2FA",
    "wwpass"
  ],
  "author": "WWPass",
  "main": "./src/lib",
  "dependencies": {
    "qrcode": "1.5.3"
  },
  "devDependencies": {
    "@babel/core": "7.22.8",
    "@babel/plugin-transform-runtime": "7.22.7",
    "@babel/preset-env": "7.22.7",
    "@size-limit/preset-big-lib": "8.2.6",
    "@size-limit/webpack-why": "8.2.6",
    "@trust/webcrypto": "0.9.2",
    "babel-loader": "9.1.3",
    "babel-plugin-rewire": "1.2.0",
    "bump-version": "0.5.0",
    "eslint": "8.44.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-jest": "27.2.2",
    "jest": "29.6.1",
    "jest-canvas-mock": "2.5.2",
    "jest-environment-jsdom": "29.6.1",
    "size-limit": "8.2.6",
    "webpack": "5.88.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "size-limit": [
    {
      "path": "./dist/wwpass-frontend.js",
      "limit": "35 Kb"
    }
  ],
  "jest": {
    "coverageDirectory": "coverage",
    "clearMocks": true,
    "verbose": true,
    "testEnvironment": "jsdom"
  }
}
