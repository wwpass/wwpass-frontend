{
  "name": "wwpass-frontend",
  "license": "MIT",
  "version": "2.0.2",
  "description": "Frontend WWPass JavaScript Library",
  "scripts": {
    "eslint": "eslint src/",
    "jest": "jest",
    "buildall": "bump patch && npm run build",
    "build": "rollup -c",
    "test": "npx size-limit ./src/index.js && npm run eslint && npm run jest",
    "bump": "bump patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwpass/wwpass-frontend.git"
  },
  "keywords": [
    "auth",
    "2FA",
    "wwpass"
  ],
  "author": "WWPass",
  "main": "./src/lib",
  "devDependencies": {
    "@trust/webcrypto": "0.9.2",
    "@babel/core": "7.3.4",
    "babel-jest": "24.3.1",
    "@babel/plugin-external-helpers": "7.2.0",
    "babel-plugin-rewire": "1.2.0",
    "@babel/preset-env": "7.3.4",
    "bump-version": "0.5.0",
    "canvas": "1.6.11",
    "del": "2.2.2",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "jest": "24.3.1",
    "regenerator-runtime": "0.12.0",
    "rollup": "1.6.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.1",
    "rollup-plugin-eslint": "5.0.0",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "6.0.2",
    "size-limit": "0.21.1",
    "text-encoding": "0.7.0"
  },
  "size-limit": [
    {
      "path": "./dist/wwpass-frontend.js",
      "limit": "18 Kb"
    }
  ],
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "qrcode": "0.8.2"
  }
}
