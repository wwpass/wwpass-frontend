{
  "name": "wwpass-frontend",
  "license": "MIT",
  "version": "3.1.2",
  "description": "Frontend WWPass JavaScript Library",
  "scripts": {
    "eslint": "eslint src/",
    "jest": "jest",
    "buildall": "bump patch && npm run build",
    "build": "rollup -c",
    "buildui": "rollup -c rollup.config.uionly.js",
    "test": "npx size-limit ./src/index.js && npm run eslint && npm run jest",
    "bump": "bump patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwpass/wwpass-frontend.git"
  },
  "keywords": [
    "auth",
    "2FA",
    "wwpass"
  ],
  "author": "WWPass",
  "main": "./src/lib",
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/preset-env": "7.3.4",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "size-limit": "^7.0.0",
    "@size-limit/preset-small-lib": "^7.0.0",
    "@trust/webcrypto": "0.9.2",
    "babel-jest": "26.0.1",
    "babel-plugin-rewire": "1.2.0",
    "bump-version": "0.5.0",
    "del": "2.2.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "eslint-plugin-jest": "26.1.4",
    "jest": "26.0.1",
    "jest-canvas-mock": "2.2.0",
    "regenerator-runtime": "0.12.0",
    "rollup": "^2.38.5",
    "rollup-plugin-terser": "^7.0.2",
    "text-encoding": "0.7.0"
  },
  "size-limit": [
    {
      "path": "./dist/wwpass-frontend.js",
      "limit": "18 Kb"
    }
  ],
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "node": "^14.3.0",
    "npm": "^8.5.2",
    "qrcode": "0.8.2"
  }
}
